generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model Carburant {
  id   Int    @id @default(autoincrement())
  type String
}

model Categories {
  id                  Int    @id @default(autoincrement())
  nom                 String
  nombre_de_vehicules Int
}

model Marques {
  id                  Int       @id @default(autoincrement())
  nom                 String
  nombre_de_vehicules Int
  Modeles             Modeles[]
}

model Modeles {
  id                  Int     @id @default(autoincrement())
  nom                 String
  marque_id           Int
  nombre_de_vehicules Int
  Marques             Marques @relation(fields: [marque_id], references: [id])
}

model Regions {
  id  Int    @id @default(autoincrement())
  nom String
}
